extends layout


block content
  .container.mt-5.background-custom
    h1.mb-4 Add a Project
    form(action="/admin/project" method="POST")
      .mb-3
        label.form-label Title:
        input.form-control(type="text" name="title")
      .mb-3
        label.form-label Description:
        textarea.form-control(name="description")
      .mb-3
        label.form-label Technologies:
        input.form-control(type="text" name="technology")
      button.btn.btn-primary(type="submit") Save Project

